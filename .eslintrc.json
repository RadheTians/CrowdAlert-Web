{
    "extends": "airbnb",
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "react/jsx-filename-extension": "off",
        "jsx-a11y/anchor-is-valid": ["error", {
            "components": ["Link"],
            "specialLink": ["to"]
        }]
    },
    "globals": {
        "window": true,
        "document": true,
        "localStorage": true,
        "formData": true,
        "fileReader": true,
        "Blob": true,
        "navigator": true
    }
}